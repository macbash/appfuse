<AdministrationConfiguration>
  <myBaseUrl>http://builds.appfuse.org</myBaseUrl>
  <myUseGzipCompression>true</myUseGzipCompression>
  <myInstanceName>AppFuse Bamboo</myInstanceName>
  <deploymentExpiryEnabled>false</deploymentExpiryEnabled>
  <myBuildExpiryConfiguration>
    <myPeriod>days</myPeriod>
    <myDuration>3</myDuration>
    <buildsToKeep>3</buildsToKeep>
    <maxIgnoredLogSize>0</maxIgnoredLogSize>
    <expiryTypeNothing>false</expiryTypeNothing>
    <expiryTypeResult>false</expiryTypeResult>
    <expiryTypeArtifact>true</expiryTypeArtifact>
    <expiryTypeBuildLog>false</expiryTypeBuildLog>
    <cronExpression>0 0 0 ? * *</cronExpression>
    <enabled>true</enabled>
  </myBuildExpiryConfiguration>
  <deploymentExpiryConfig>
    <expiryEnabled>true</expiryEnabled>
    <expiryTypes enum-type="com.atlassian.bamboo.deployments.expiry.DeploymentExpiryType">DEPLOYMENT_VERSION_ARTIFACTS_EXPIRY</expiryTypes>
    <expiryPeriod>P3D</expiryPeriod>
    <deploymentsToKeep>2</deploymentsToKeep>
    <maxIgnoredLogSize>-1</maxIgnoredLogSize>
  </deploymentExpiryConfig>
  <myBuildHangingConfig>
    <multiplier>2.5</multiplier>
    <minutesBetweenLogs>5</minutesBetweenLogs>
    <minutesBeforeQueueTimeout>0</minutesBeforeQueueTimeout>
    <disabled>false</disabled>
  </myBuildHangingConfig>
  <concurrentBuildConfig>
    <enabled>false</enabled>
    <numberConcurrentBuilds>1</numberConcurrentBuilds>
  </concurrentBuildConfig>
  <dashboardPageSize>50</dashboardPageSize>
  <enableSignup>true</enableSignup>
  <enableAnonymousAccess>true</enableAnonymousAccess>
  <enableExternalUserManagement>false</enableExternalUserManagement>
  <enableViewContactDetails>false</enableViewContactDetails>
  <enableRestrictedAdmin>false</enableRestrictedAdmin>
  <remoteAgentFunctionEnabled>true</remoteAgentFunctionEnabled>
  <remoteAgentAuthenticationEnabled>true</remoteAgentAuthenticationEnabled>
  <auditLoggingEnabled>false</auditLoggingEnabled>
  <gravatarSupportEnabled>true</gravatarSupportEnabled>
  <gravatarServerUrl>https://secure.gravatar.com/avatar/</gravatarServerUrl>
  <currentApplication>
    <applicationId>7F000001013B890CBA55952000D0708E</applicationId>
    <publicKey></publicKey>
    <privateKey></privateKey>
  </currentApplication>
  <captchaConfiguration>
    <enableCaptcha>true</enableCaptcha>
    <enableCaptchaOnSignup>true</enableCaptchaOnSignup>
    <maxLoginAttempts>3</maxLoginAttempts>
  </captchaConfiguration>
  <systemProperties>
    <entry>
      <string>AutoFavouriteEnabled</string>
      <string>true</string>
    </entry>
  </systemProperties>
  <globalVariables/>
  <buildHangedInQueueNotificationTimeout>0</buildHangedInQueueNotificationTimeout>
  <repositorySettings>
    <useNtlmAuth>false</useNtlmAuth>
  </repositorySettings>
  <artifactHandlerConfiguration>
    <entry>
      <string>custom.artifactHandlers.comAtlassianBambooPluginArtifactHandlerLocal:ServerLocalArtifactHandler:enabledForNonShared</string>
      <string>true</string>
    </entry>
    <entry>
      <string>custom.artifactHandlers.comAtlassianBambooPluginArtifactHandlerLocal:ServerLocalArtifactHandler:enabledForShared</string>
      <string>true</string>
    </entry>
    <entry>
      <string>custom.artifactHandlers.comAtlassianBambooPluginArtifactHandlerRemote:BambooRemoteArtifactHandler:enabledForNonShared</string>
      <string>true</string>
    </entry>
    <entry>
      <string>custom.artifactHandlers.comAtlassianBambooPluginArtifactHandlerRemote:BambooRemoteArtifactHandler:enabledForShared</string>
      <string>true</string>
    </entry>
  </artifactHandlerConfiguration>
  <branchDetectionCheckInterval>300</branchDetectionCheckInterval>
  <xsrfProtectionEnabled>false</xsrfProtectionEnabled>
  <xsrfAllowsMutativeGets>true</xsrfAllowsMutativeGets>
  <enableTestQuarantine>true</enableTestQuarantine>
</AdministrationConfiguration>